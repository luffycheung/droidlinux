    <string name="tutorial_title">Touchpad tutorial</string>
    <string name="tutorial_introduction_text">This tutorial will show you how to use the touchpad. If you want you can directly test the gestures inside the tutorial.</string>
    <string name="tutorial_move_title">Move pointer</string>
    <string name="tutorial_move_text">Touch and move to move the mouse pointer.</string>
    <string name="tutorial_1f_tap_title">Left mouse button</string>
    <string name="tutorial_1f_tap_text">Tap with one-finger</string>
    <string name="tutorial_2f_tap_title">Right mouse button</string>
    <string name="tutorial_2f_tap_text">Tap with two-fingers</string>
    <string name="tutorial_3f_tap_title">Middle mouse button</string>
    <string name="tutorial_3f_tap_text">Tap with three-fingers</string>
    <string name="tutorial_drag_title">Drag</string>
    <string name="tutorial_drag_text">Tap and quickly touch to start a drag action.</string>
    <string name="tutorial_2f_scroll_title">Scroll</string>
    <string name="tutorial_2f_scroll_text">Slide two fingers in any direction to scroll.</string>
    <string name="tutorial_1f_scroll_title">One-finger scroll</string>
    <string name="tutorial_1f_scroll_text">You can also scroll if you slide up or down on the right side of the touchpad.</string>
    <string name="tutorial_pinch_zoom_title">Pinch to zoom</string>
    <string name="tutorial_pinch_zoom_text">Pinch with two fingers to zoom in or out.</string>
    <string name="tutorial_3f_lr_navigate_title">Back and Forward</string>
    <string name="tutorial_3f_lr_navigate_text">Swipe left or right with three fingers to move back and forward in some applications.</string>
    <string name="tutorial_3f_d_desktop_title">Show desktop</string>
    <string name="tutorial_3f_d_desktop_text">Swipe down with three fingers to show the desktop.</string>
    <string name="tutorial_4f_lr_snap_window_title">Snap window left or right</string>
    <string name="tutorial_4f_lr_snap_window_text">Swipe left or right with four fingers to snap the current window to the left or right side of the screen.</string>
    <string name="tutorial_4f_ud_max_min_window_title">Maximize/minimize window</string>
    <string name="tutorial_4f_ud_max_min_window_text">Swipe up or down with four fingers to maximize or minimize the current window.</string>
    <string name="tutorial_3f_u_android_title">Home</string>
    <string name="tutorial_3f_u_android_text">Swipe up with three fingers to show the home screen.</string>
    <string name="tutorial_3f_l_android_title">Back</string>
    <string name="tutorial_3f_l_android_text">Swipe left with three fingers to navigate back.</string>
    <string name="tutorial_3f_d_gnome_shell_title">Activities overview</string>
    <string name="tutorial_3f_d_gnome_shell_text">Swipe down with three fingers to show or hide the activities overview.</string>
    <string name="tutorial_3f_u_gnome_shell_title">Message tray</string>
    <string name="tutorial_3f_u_gnome_shell_text">Swipe up with three fingers to show or hide the message tray.</string>
    <string name="tutorial_4f_ud_gnome_shell_title">Change workspace</string>
    <string name="tutorial_4f_ud_gnome_shell_text">Swipe up or down with four fingers to change the workspace.</string>
    <string name="tutorial_4f_u_osx_title">Mission Control</string>
    <string name="tutorial_4f_u_osx_text">Swipe up with four fingers to get a bird’s-eye view of everything running on your Mac.</string>
    <string name="tutorial_4f_d_osx_title">App Exposé</string>
    <string name="tutorial_4f_d_osx_text">Swipe down with four fingers to see all of your open apps.</string>
    <string name="tutorial_4f_lr_osx_title">Change workspace</string>
    <string name="tutorial_4f_lr_osx_text">Swipe left or right with four fingers to change the workspace.</string>
    <string name="tutorial_3f_d_ubuntu_unity_title">Open Launcher</string>
    <string name="tutorial_3f_d_ubuntu_unity_text">Swipe down with three fingers to show or hide the Launcher.</string>
    <string name="tutorial_3f_u_ubuntu_unity_title">Workspace Switcher</string>
    <string name="tutorial_3f_u_ubuntu_unity_text">Swipe up with three fingers to show or hide the Workspace Switcher.</string>
    <string name="tutorial_3f_u_windows7_title">Start menu</string>
    <string name="tutorial_3f_u_windows7_text">Swipe up with three fingers to show the Windows start menu.</string>
    <string name="tutorial_edge_l_windows8_title">Switch applications</string>
    <string name="tutorial_edge_l_windows8_text">Swipe with one finger from the left edge to switch to the last application.</string>
    <string name="tutorial_edge_r_windows8_title">Charms menu</string>
    <string name="tutorial_edge_r_windows8_text">Swipe with one finger from the right edge to open the charms menu.</string>
    <string name="tutorial_edge_t_windows8_title">Drag window</string>
    <string name="tutorial_edge_t_windows8_text">Swipe with one finger from the top edge to drag a full-screen window.</string>
    <string name="tutorial_edge_b_windows8_title">Application menu</string>
    <string name="tutorial_edge_b_windows8_text">Swipe with one finger from the bottom edge to open the application menu.</string>
    <string name="tutorial_3f_u_windows8_title">Start screen</string>
    <string name="tutorial_3f_u_windows8_text">Swipe up with three fingers to show or hide the Windows start screen.</string>
